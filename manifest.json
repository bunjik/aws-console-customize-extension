{
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "description": "__MSG_ext_description__",
  "version": "0.2",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["https://*.console.aws.amazon.com/*"],
      "js": ["js/jquery.min.js", "extension.js"],
      "css": ["css/extension.css"]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons" : {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
